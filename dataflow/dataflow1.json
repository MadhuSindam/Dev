{
	"name": "dataflow1",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"linkedService": {
						"referenceName": "AzureSqlDatabase_Emp_DB_Test_123",
						"type": "LinkedServiceReference"
					},
					"name": "EmployeeDataset"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "DS_adfcontainer_output",
						"type": "DatasetReference"
					},
					"name": "ADLOutputSink"
				}
			],
			"transformations": [
				{
					"name": "select1"
				},
				{
					"name": "join1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          EmployeeID as integer,",
				"          FirstName as string,",
				"          LastName as string,",
				"          Email as string,",
				"          PhoneNumber as string,",
				"          HireDate as date,",
				"          DepartmentID as integer,",
				"          Salary as decimal(10,2),",
				"          MANAGERID as integer",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     format: 'table',",
				"     store: 'sqlserver',",
				"     schemaName: 'dbo',",
				"     tableName: 'Employees',",
				"     isolationLevel: 'READ_UNCOMMITTED') ~> EmployeeDataset",
				"join1 select(mapColumn(",
				"          EmployeeID,",
				"          FirstName,",
				"          LastName,",
				"          Email,",
				"          PhoneNumber,",
				"          HireDate,",
				"          DepartmentID,",
				"          Salary,",
				"          MANAGERID",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select1",
				"EmployeeDataset join(ERROR_FUNCTION('') == ERROR_FUNCTION(''),",
				"     joinType:'inner',",
				"     matchType:'exact',",
				"     ignoreSpaces: false,",
				"     broadcast: 'auto')~> join1",
				"select1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          id as string,",
				"          name as string,",
				"          dob as string,",
				"          email as string,",
				"          gender as string,",
				"          country as string,",
				"          region as string,",
				"          city as string,",
				"          asset as string,",
				"          marital_status as string",
				"     ),",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> ADLOutputSink"
			]
		}
	}
}