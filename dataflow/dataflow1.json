{
	"name": "dataflow1",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "AzureSqlTable1",
						"type": "DatasetReference"
					},
					"name": "EmployeeDataset"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "DS_adfcontainer_output",
						"type": "DatasetReference"
					},
					"name": "ADLOutputSink"
				}
			],
			"transformations": [],
			"scriptLines": [
				"source(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> EmployeeDataset",
				"EmployeeDataset sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          id as string,",
				"          name as string,",
				"          dob as string,",
				"          email as string,",
				"          gender as string,",
				"          country as string,",
				"          region as string,",
				"          city as string,",
				"          asset as string,",
				"          marital_status as string",
				"     ),",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> ADLOutputSink"
			]
		}
	}
}